<%- model_class = MsGender -%>
<div class="page-header">
  <h1><%=t '.title', :default => MsGender %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:name) %></dt>
      <dd><pre class="prettyprint"><%= @ms_gender.name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:is_active) %></dt>
      <dd><pre class="prettyprint"><%= @ms_gender.is_active %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              ms_genders_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_ms_gender_path(@ms_gender), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              ms_gender_path(@ms_gender),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

